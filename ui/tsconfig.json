{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": [
      "es2019",
      "es2020.promise",
      "es2020.bigint",
      "es2020.string",
      "DOM"
    ],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "strict": true,
    "target": "es2019",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "allowJs": false,
    "forceConsistentCasingInFileNames": true
  }
}
